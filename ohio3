local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "key input", HidePremium = false, SaveConfig = true, ConfigFolder = "key input"})

getgenv().TPPlayer = "string"


local TesTab = Window:MakeTab({
	Name = "key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = TesTab:AddSection({
	Name = "enter key"
})

TesTab:AddTextbox({
	Name = "key input",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
      getgenv().TPPlayer = Value
	end	  
})

TesTab:AddButton({
	Name = "enter",
	Callback = function()
if getgenv().TPPlayer == "script aufa" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/aufa0607/p/main/ohio4"))();
elseif getgenv().TPPlayer == "speed city script" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/aufa0607/p/main/speed%20city"))();
      end
if getgenv().TPPlayer == "dfgh" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/NukeVsCity/TheALLHACKLoader/main/NukeLoader"))()
elseif getgenv().TPPlayer == "infinity yeld" then
loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))();
      end
    end
})
