local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "aufa v1.2", HidePremium = false, 
IntroText = "aufa aluna alisha", SaveConfig = true, ConfigFolder = "aufa v1.2"})

getgenv().TPPlayer = "string"


local TutTab = Window:MakeTab({
	Name = "main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = TutTab:AddSection({
	Name = "script section"
})

TutTab:AddButton({
	Name = "antilag",
	Callback = function(Value)
      		print("suscribe")
  	end    
})

TutTab:AddButton({
	Name = "noclip",
	Callback = function(Value)
      		print("suscribe")
loadstring(game:HttpGet("https://raw.githubusercontent.com/aufa0607/p/main/p"))();
