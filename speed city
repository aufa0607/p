local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Speed City Farming 2.0", "BloodTheme")
local Tab1 = Window:NewTab("Ghost City")
local Tab3 = Window:NewTab("Trillionaire City")
local Tab2 = Window:NewTab("Misc")
local Tab4 = Window:NewTab("Credits")
local Section4 = Tab1:NewSection("Orb Farm")
local Section5 = Tab1:NewSection("Crate Farm")
local Section6 = Tab1:NewSection("Required Achievement Crate Farm")
local Section8 = Tab2:NewSection("Teleports")
local Section9 = Tab2:NewSection("Extra")
local Section10 = Tab3:NewSection("Farming (Section created for Caleb Gray on Youtube)")
local Section11 = Tab4:NewSection("xHeptc - most of scripting + UI")
local Section12 = Tab4:NewSection("Counter#2747 - scripted brand new features")
Section10:NewLabel("You must have Trillionaire City unlocked.")
 
 
Section9:NewButton("Claim Cloud Trail", "TP to Cloud Trail", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(3, 659, 0)
end)
 
 
Section4:NewToggle("White Orbs", "Teleports You To The White Orbs", function(state)
if state then
getgenv().Farming1 = true
		while Farming1 == true do
            if game:GetService("Workspace").GhostCity.GhostOrbsMega:FindFirstChild("GhostMega") then 
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").GhostCity.GhostOrbsMega.GhostMega.Outside.Position)
end
			wait()
		end
else
getgenv().Farming1 = false
end
end)
 
Section4:NewToggle("Black Orbs", "Teleports You To The Black Orbs", function(state)
if state then
getgenv().Farming1 = true
